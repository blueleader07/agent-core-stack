{
  "name": "agentcore-runtime-container",
  "version": "1.0.0",
  "description": "AWS Bedrock AgentCore Runtime container with LangGraph agent",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch server.ts",
    "docker:build": "docker build --platform linux/arm64 -t agentcore-runtime .",
    "docker:run": "docker run -p 8080:8080 --env-file .env agentcore-runtime",
    "docker:test": "curl http://localhost:8080/ping"
  },
  "dependencies": {
    "@langchain/aws": "^1.2.0",
    "@langchain/core": "^1.1.15",
    "@langchain/langgraph": "^1.1.0",
    "axios": "^1.7.0",
    "express": "^4.21.0",
    "ws": "^8.18.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.0.0",
    "@types/ws": "^8.5.0",
    "tsx": "^4.21.0",
    "typescript": "~5.2.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
